void texto(String texto, uint8_t x, uint8_t y, uint8_t r, uint8_t g, uint8_t b) {
  for (uint8_t caracter = 0; caracter < texto.length(); caracter++) {
    char ascii = texto.charAt(caracter) - 32;
    for (uint8_t row = 0; row < 5; row++) {
      uint8_t col = chars[ascii][row];
      if (bitRead(col, 2)) {
        matrix.drawPixel(x + 0 + caracter * 4, y + row, matrix.Color(r, g, b));
      } else {
        matrix.drawPixel(x + 0 + caracter * 4, y + row, matrix.Color(0, 0, 0));
      }
      if (bitRead(col, 1)) {
        matrix.drawPixel(x + 1 + caracter * 4, y + row, matrix.Color(r, g, b));
      } else {
        matrix.drawPixel(x + 1 + caracter * 4, y + row, matrix.Color(0, 0, 0));
      }
      if (bitRead(col, 0)) {
        matrix.drawPixel(x + 2 + caracter * 4, y + row, matrix.Color(r, g, b));
      } else {
        matrix.drawPixel(x + 2 + caracter * 4, y + row, matrix.Color(0, 0, 0));
      }
    }
  }
  matrix.show();
}


